<Application x:Class="iEventViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:iEventViewer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:DateConverter x:Key="DateConverter" />
        
        <LinearGradientBrush x:Key="HeaderBackground" StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0" Color="#4C94C6" />
            <GradientStop Offset="1" Color="#185CA2" />
        </LinearGradientBrush>
    </Application.Resources>
</Application>
